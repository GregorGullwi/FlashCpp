// Minimal cstddef header stub for FlashCpp
// Provides only the basic type definitions needed for compilation
// This is a simplified stub - the real libstdc++/libc++ headers are too complex for FlashCpp to parse

#ifndef _FLASHCPP_CSTDDEF
#define _FLASHCPP_CSTDDEF

namespace std {
    typedef unsigned long size_t;
    typedef long ptrdiff_t;
}

#endif // _FLASHCPP_CSTDDEF
